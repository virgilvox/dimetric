{
  "name": "@dimetric/formats",
  "version": "0.0.1",
  "description": "Format parsers and writers for Dimetric: native JSON, Tiled TMJ/TMX, Aseprite, TexturePacker",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "https://github.com/dimetric/dimetric",
    "directory": "packages/formats"
  },
  "keywords": ["isometric", "tilemap", "tiled", "tmx", "tmj", "aseprite", "texturepacker"],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.7",
    "pako": "^2.1.0"
  },
  "peerDependencies": {
    "@dimetric/core": "workspace:*"
  },
  "devDependencies": {
    "@dimetric/core": "workspace:*",
    "@types/pako": "^2.0.4",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
